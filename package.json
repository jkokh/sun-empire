{
  "name": "band.ink",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev-all": "cross-env NODE_ENV=local concurrently \"npm run start\" \"npm run dev --prefix ./frontend\"",
    "build-all": "concurrently \"npm run build\" \"npm run build --prefix ./frontend\"",
    "copy:views": "copyfiles -u 1 'src/views/*' 'src/views/**/*' 'src/templates/*' 'src/templates/**/*' dist",
    "clean": "rimraf dist",
    "copy:frontend": "copyfiles -u 2 'frontend/dist/public/**/*' dist/public && copyfiles -u 3 'frontend/dist/public/.vite/*' dist/public/.vite",
    "build": "npm run clean && npm run prisma:generate && tsc && npm run copy:views && npm run copy:frontend",
    "start": "tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "prisma:create": "npx prisma migrate dev --name create --create-only",
    "prisma:deploy": "npx prisma migrate deploy",
    "lint": "eslint --ext .ts,tsx --ignore-path .gitignore .",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:debug": "playwright test --debug",
    "test:single": "playwright test --grep"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.470.0",
    "@prisma/client": "^5.10.2",
    "@tsed/ajv": "^7.87.1",
    "@tsed/common": "^7.87.1",
    "@tsed/core": "^7.87.1",
    "@tsed/di": "^7.87.1",
    "@tsed/engines": "^7.87.1",
    "@tsed/exceptions": "^7.87.1",
    "@tsed/json-mapper": "^7.87.1",
    "@tsed/logger": "^6.7.3",
    "@tsed/logger-file": "^6.7.3",
    "@tsed/openspec": "^7.87.1",
    "@tsed/platform-cache": "^7.87.1",
    "@tsed/platform-exceptions": "^7.87.1",
    "@tsed/platform-express": "^7.87.1",
    "@tsed/platform-log-middleware": "^7.87.1",
    "@tsed/platform-middlewares": "^7.87.1",
    "@tsed/platform-params": "^7.87.1",
    "@tsed/platform-response-filter": "^7.87.1",
    "@tsed/platform-views": "^7.87.1",
    "@tsed/prisma": "^7.87.1",
    "@tsed/schema": "^7.87.1",
    "@tsed/swagger": "^7.87.1",
    "ajv": "^8.12.0",
    "bcryptjs": "^2.4.3",
