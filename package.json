    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^4.0.1",
    "express": "^4.18.2",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "multer": "^2.0.1",
    "nodemailer": "^6.9.7",
    "pug": "^3.0.2",
    "uuid": "^9.0.1"
